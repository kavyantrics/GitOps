# Production inventory example
# Copy to prod.yml and fill in your EC2 instance IPs

all:
  children:
    jenkins:
      hosts:
        jenkins-server:
          ansible_host: 1.2.3.4
          ansible_user: ubuntu
          ansible_ssh_private_key_file: ~/.ssh/gitops-key.pem

    monitoring:
      hosts:
        monitoring-server:
          ansible_host: 5.6.7.8
          ansible_user: ubuntu
          ansible_ssh_private_key_file: ~/.ssh/gitops-key.pem

    kops:
      hosts:
        kops-master:
          ansible_host: 9.10.11.12
          ansible_user: ubuntu
          ansible_ssh_private_key_file: ~/.ssh/gitops-key.pem
        kops-worker:
          ansible_host: 13.14.15.16
          ansible_user: ubuntu
          ansible_ssh_private_key_file: ~/.ssh/gitops-key.pem

